      <type>
        <code value="Reference" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Patient" />
      </type>
    </element>
    <element id="MedicationAdministration.performer">
      <path value="MedicationAdministration.performer" />
      <short value="The practioner(s) who performed the administration." />
      <min value="1" />
    </element>
    <element id="MedicationAdministration.prescription">
      <path value="MedicationAdministration.prescription" />
      <short value="The MedicationRequest (i.e. Prescription) that this MedicationStatement is performed against." />
      <min value="1" />
